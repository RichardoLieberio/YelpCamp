<%- layout("./templates/boilerplate") %>
<div class="alert alert-danger" role="alert">
    <h4 class="alert-heading"><%= error.statusCode %> - <%= error.message %></h4>
    <% if (typeof(error.stack) === "object") { %>
        <% error.stack.forEach(function(stack) { %>
            <p class="m-0"><%= stack %></p>
        <% }); %>
    <% } else { %>
        <p class="m-0"><%= error.stack %></p>
    <% } %>
</div>